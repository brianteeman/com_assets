<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" default="0" label="COM_ASSETS_FORM_LBL_ASSET_CREATED_BY" description="COM_ASSETS_FORM_DESC_ASSET_CREATED_BY" hidden="true" hint="COM_ASSETS_FORM_LBL_ASSET_CREATED_BY"/>
    <field name="modified_by" default="0" label="COM_ASSETS_FORM_LBL_ASSET_MODIFIED_BY" description="COM_ASSETS_FORM_DESC_ASSET_MODIFIED_BY" hidden="true" hint="COM_ASSETS_FORM_LBL_ASSET_MODIFIED_BY"/>
    <field name="time_created" type="timecreated" required="true" label="COM_ASSETS_FORM_LBL_ASSET_TIME_CREATED" description="COM_ASSETS_FORM_DESC_ASSET_TIME_CREATED" hint="COM_ASSETS_FORM_LBL_ASSET_TIME_CREATED"/>
    <field name="path" type="text" required="true" label="COM_ASSETS_FORM_LBL_ASSET_PATH" description="COM_ASSETS_FORM_DESC_ASSET_PATH" hint="COM_ASSETS_FORM_LBL_ASSET_PATH"/>
    <field name="type" type="list" label="COM_ASSETS_FORM_LBL_ASSET_TYPE" description="COM_ASSETS_FORM_DESC_ASSET_TYPE" class="inputbox" required="true" default="1">
      <option value="1">Javascript</option>
      <option value="2">Style Sheet</option>
    </field>
    <field name="regex_pages" type="textarea" cols="60" rows="6" filter="raw" label="COM_ASSETS_FORM_LBL_ASSET_REGEX_PAGES" description="COM_ASSETS_FORM_DESC_ASSET_REGEX_PAGES" hint="COM_ASSETS_FORM_LBL_ASSET_REGEX_PAGES"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
